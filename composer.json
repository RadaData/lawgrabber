{
  "name": "openrada/lawgrabber",
  "description": "The ukrainian laws grabber.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "vlucas/phpdotenv": "~1.0",
    "aws/aws-sdk-php": "~3.0",
    "symfony/dom-crawler": "2.7.*",
    "jonnyw/php-phantomjs": "3.*",
    "symfony/css-selector": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "fzaninotto/faker": "~1.0",
    "barryvdh/laravel-ide-helper": "*",
    "doctrine/dbal": "~2.3",
    "illuminate/filesystem": "5.1.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "LawGrabber\\": "LawGrabber"
    },
    "classmap": [
      "database/"
    ],
    "files": [
      "LawGrabber/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "bin-dir": "./bin",
    "preferred-install": "dist"
  },
  "scripts": {
    "post-install-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS"
    ],
    "post-update-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS"
    ]
  }
}
